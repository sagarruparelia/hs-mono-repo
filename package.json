{
  "name": "@hs-mono-repo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --projects=mfe-profile,mfe-summary,web-cl,web-hs --parallel=4",
    "start:mfe": "nx run-many --target=serve --projects=mfe-profile,mfe-summary --parallel=2",
    "start:shells": "nx run-many --target=serve --projects=web-cl,web-hs --parallel=2",
    "start:web-cl": "nx serve web-cl",
    "start:web-hs": "nx serve web-hs",
    "start:profile": "nx serve mfe-profile",
    "start:summary": "nx serve mfe-summary",
    "start:bff": "nx serve bff",
    "build": "nx run-many --target=build --all --configuration=production",
    "build:mfe": "nx run-many --target=build --projects=mfe-profile,mfe-summary --configuration=production",
    "build:shells": "nx run-many --target=build --projects=web-cl,web-hs --configuration=production",
    "build:web-cl": "nx build web-cl --configuration=production",
    "build:web-hs": "nx build web-hs --configuration=production",
    "build:profile": "nx build mfe-profile --configuration=production",
    "build:summary": "nx build mfe-summary --configuration=production",
    "build:bff": "nx build bff",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "test:web-cl": "nx test web-cl",
    "test:web-hs": "nx test web-hs",
    "test:profile": "nx test mfe-profile",
    "test:summary": "nx test mfe-summary",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "lint:web-cl": "nx lint web-cl",
    "lint:web-hs": "nx lint web-hs",
    "lint:profile": "nx lint mfe-profile",
    "lint:summary": "nx lint mfe-summary",
    "typecheck": "nx run-many --target=typecheck --all",
    "typecheck:web-cl": "nx typecheck web-cl",
    "typecheck:web-hs": "nx typecheck web-hs",
    "typecheck:profile": "nx typecheck mfe-profile",
    "typecheck:summary": "nx typecheck mfe-summary",
    "e2e": "nx run-many --target=e2e --all",
    "e2e:web-cl": "nx e2e web-cl-e2e",
    "e2e:web-hs": "nx e2e web-hs-e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "graph": "nx graph",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "affected:e2e": "nx affected --target=e2e",
    "affected:graph": "nx affected:graph",
    "clean": "nx reset",
    "clean:dist": "rm -rf dist",
    "clean:cache": "nx reset && rm -rf node_modules/.cache",
    "clean:all": "nx reset && rm -rf dist node_modules",
    "reset": "nx reset",
    "update": "nx migrate latest",
    "update:check": "nx migrate latest --interactive",
    "prepare": "husky install || true",
    "deploy:profile": "./scripts/deploy-mfe.sh profile $(node -p \"require('./package.json').version\") ${DEPLOY_ENV:-staging}",
    "deploy:summary": "./scripts/deploy-mfe.sh summary $(node -p \"require('./package.json').version\") ${DEPLOY_ENV:-staging}",
    "deploy:web-cl": "./scripts/deploy-mfe.sh web-cl $(node -p \"require('./package.json').version\") ${DEPLOY_ENV:-staging}",
    "deploy:web-hs": "./scripts/deploy-mfe.sh web-hs $(node -p \"require('./package.json').version\") ${DEPLOY_ENV:-staging}",
    "deploy:all": "npm run deploy:profile && npm run deploy:summary && npm run deploy:web-cl && npm run deploy:web-hs",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "terraform:init": "cd terraform && terraform init",
    "terraform:plan": "cd terraform && terraform plan",
    "terraform:apply": "cd terraform && terraform apply",
    "terraform:destroy": "cd terraform && terraform destroy"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@module-federation/vite": "^1.9.0",
    "@nx/devkit": "22.0.2",
    "@nx/eslint": "22.0.2",
    "@nx/eslint-plugin": "22.0.2",
    "@nx/js": "22.0.2",
    "@nx/playwright": "22.0.2",
    "@nx/react": "^22.0.2",
    "@nx/vite": "^22.0.2",
    "@nx/web": "22.0.2",
    "@nx/workspace": "22.0.2",
    "@nxrocks/nx-spring-boot": "^11.0.1",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@types/node": "20.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "22.0.2",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.0.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.134.9",
    "@tanstack/router-devtools": "^1.134.9",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
